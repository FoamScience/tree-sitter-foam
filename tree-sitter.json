{
  "grammars": [
    {
      "name": "foam",
      "camelcase": "Foam",
      "scope": "source.foam",
      "path": ".",
      "file-types": [
        "foam",
        "OpenFOAM"
      ],
      "injection-regex": "^(foam|OpenFOAM)$",
      "content-regex": "FoamFile",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "0.4.1",
    "license": "MIT",
    "description": "Tree sitter grammer for OpenFOAM dictionaries",
    "authors": [
      {
        "name": "Mohammed Elwardi Fadeli"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-foam"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
